#include <iostream>
using namespace std;
int main()
{
    int nilai[5];
    nilai[0] = 23;
    nilai[1] = 50;
    nilai[2] = 34;
    nilai[3] = 78;
    nilai[4] = 90;

    cout << "Isi Array pertama :" << nilai[0] <<endl;
    cout << "Isi Array kedua :" << nilai[1] <<endl;
    cout << "Isi Array ketiga :" << nilai[2] <<endl;
    cout << "Isi Array keempat :" << nilai[3] <<endl;
    cout << "Isi Array kelima :" << nilai[4] <<endl;
    return 0;
}
