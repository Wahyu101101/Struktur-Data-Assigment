#include <iostream>
#include <string>
using namespace std;

class Mahasiswa {
    private:
    string nama;
    int umur;
    
    public:
    Mahasiswa(string n, int u){
        nama = n;
        umur = u;
    }


void tampilkanData(){
    cout << "Nama : " << nama << ", Umur: " << umur << " tahun" <<endl;
}
};

int main(){
    Mahasiswa mhs1("Wahyu Hidayat", 20);
    Mahasiswa mhs2("Nor Jatil Hasanah", 21
    );

    mhs1.tampilkanData();
    mhs2.tampilkanData();
    return 0;
}
